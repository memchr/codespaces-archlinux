	"image": "ghcr.io/archlinux/archlinux:base-devel",
	"features": {
		"./features/common": {},
		"./features/git": {},
		"./features/base-devel": {},
		"./features/llvm": {},
		"./features/node": {},
		"./features/python": {},
		"./features/go": {},
		"./features/ssh": {},
		"./features/github": {
			"useAct": true
